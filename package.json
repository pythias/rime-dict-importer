{
  "name": "rime-dict-manager",
  "productName": "Rime Dict Manager",
  "description": "中州韵词库管理",
  "license": "SEE LICENSE IN LICENSE",
  "repository": {
    "type": "git",
    "url": "https://github.com/pythias/rime-dict-importer.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "keywords": [],
  "main": "./main.js",
  "version": "1.3.0",
  "author": "pythias",
  "scripts": {
    "start": "electron .",
    "dev": "electron . --debug",
    "dist": "npm-run-all dist:*",
    "dist:win": "electron-builder --win --x64",
    "dist:mac": "electron-builder --mac --x64",
    "package": "npm-run-all package:*",
    "package:mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --out=out --icon=build/icon.icns",
    "package:win": "electron-packager . --overwrite --platform=win32 --arch=ia32 --out=out --icon=build/icon.ico"
  },
  "build": {
    "appId": "com.duo.rime",
    "mac": {
      "target": [
        "dmg",
        "zip"
      ]
    },
    "win": {
      "target": [
        "nsis",
        "zip"
      ]
    }
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.3",
    "electron-dl": "^3.0.0",
    "electron-log": "^2.2.17",
    "electron-settings": "^3.0.7",
    "js-yaml": "^3.13.1",
    "md5": "^2.2.1",
    "request": "^2.88.0"
  },
  "devDependencies": {
    "devtron": "^1.3.0",
    "electron": "^7.1.7",
    "electron-builder": "^22.3.2"
  }
}
